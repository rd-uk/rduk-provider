{
  "name": "rduk-provider",
  "version": "1.0.0",
  "description": "Easily add providers to your Node.js app",
  "main": "lib/factory.js",
  "scripts": {
    "lint": "jshint lib/*.js examples/*.js spec/*.js",
    "pretest": "npm run lint && cp ./examples/*.yml .",
    "test": "istanbul cover node_modules/jasmine/bin/jasmine.js --report cobertura",
    "posttest": "rm -rf ./config*yml && istanbul report && cat ./coverage/lcov.info | ./node_modules/coveralls/bin/coveralls.js && rm -rf ./coverage"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rd-uk/rduk-provider.git"
  },
  "keywords": [
    "provider"
  ],
  "author": "Kim UNG",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/rd-uk/rduk-provider/issues"
  },
  "homepage": "https://github.com/rd-uk/rduk-provider#readme",
  "devDependencies": {
    "coveralls": "2.13.0",
    "istanbul": "0.4.5",
    "jasmine": "2.6.0",
    "jshint": "2.9.5"
  },
  "dependencies": {
    "lodash": "4.17.3",
    "rduk-configuration": "1.2.0",
    "rduk-errors": "1.2.3"
  }
}
